package 사전학습;

import java.util.Scanner;

public class Q2063_D1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int num = in.nextInt();
		
		int[] array = new int[num];
		for(int i=0; i<num; i++) {
			array[i] = in.nextInt();
		}
		
		//중간값의 인덱스 = num/2;
		System.out.println(array[num/2]);
	}

}
