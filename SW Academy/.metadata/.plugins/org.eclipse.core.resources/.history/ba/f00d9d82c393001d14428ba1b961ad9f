package 사전학습;

import java.util.Scanner;

public class Q1936_D1 {

	public static void main(String[] args) {
		//가위는 1, 바위는 2, 보는 3
		//1>3이면 이김 (반대!)
		//2>1이면 이김
		//3>2이면 이김  
		
		Scanner in = new Scanner(System.in);
		int A = in.nextInt();
		int B = in.nextInt();
		
		if((A==1 || B ==1) && (A == 3 || B ==3)) {
			System.out.println(Math.min(A, B));
		}
		System.out.print(Math.max(A, B));


	}

}
